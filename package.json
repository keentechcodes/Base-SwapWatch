    "chalk": "^4.1.2",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "ioredis": "^5.7.0",
    "redis": "^5.8.2"
  },
  "devDependencies": {
    "@cloudflare/next-on-pages": "^1.13.16",
    "@cloudflare/vitest-pool-workers": "^0.9.10",
    "@cloudflare/workers-types": "^4.20250927.0",
    "@types/express": "^5.0.3",
    "@types/ioredis": "^4.28.10",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.3.0",
