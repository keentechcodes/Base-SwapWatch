{
  "name": "baseproject",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon --exec ts-node src/server.ts",
    "dev:worker": "wrangler dev src/worker/index.ts",
    "dev:pages": "next dev",
    "start": "node dist/server.js",
    "build": "node scripts/build.js",
    "build:worker": "tsc --noEmit src/worker/index.ts",
    "build:pages": "next build",
    "deploy:worker": "wrangler deploy",
    "deploy:pages": "vercel --prod",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "clean": "rm -rf dist coverage .next .vercel",
    "typecheck": "tsc --noEmit",
    "lint": "tsc --noEmit",
    "validate-apis": "node scripts/validate-apis.js",
    "webhook": "node scripts/cdp-webhook-manager.js",
    "webhook:list": "node scripts/cdp-webhook-manager.js list",
    "webhook:create": "node scripts/cdp-webhook-manager.js create",
    "webhook:update": "node scripts/cdp-webhook-manager.js update",
    "webhook:delete": "node scripts/cdp-webhook-manager.js delete",
    "webhook:monitor": "node scripts/cdp-webhook-manager.js monitor",
    "webhook:activate": "node scripts/cdp-webhook-manager.js activate",
    "webhook:deactivate": "node scripts/cdp-webhook-manager.js deactivate",
    "cf:login": "wrangler login",
    "cf:whoami": "wrangler whoami",
    "cf:tail": "wrangler tail"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
