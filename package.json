{
  "name": "swapwatch",
  "version": "1.0.0",
  "description": "Real-time cryptocurrency swap monitoring platform on Cloudflare edge",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:worker\" \"npm run dev:pages\"",
    "dev:worker": "wrangler dev src/worker/index.ts",
    "dev:pages": "next dev",
    "start": "node dist/server.js",
    "build": "next build",
    "build:worker": "tsc --noEmit src/worker/index.ts",
    "build:pages": "next build && npx @cloudflare/next-on-pages",
    "build:all": "npm run build:worker && npm run build:pages",
    "deploy:worker": "wrangler deploy --env production",
    "deploy:pages": "npm run build:pages && wrangler pages deploy .vercel/output/static --project-name swapwatch",
    "test": "npm run test:worker && npm run test:frontend",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "clean": "rm -rf dist coverage .next .vercel",
    "typecheck": "tsc --noEmit",
    "lint": "next lint",
    "validate-apis": "node scripts/validate-apis.js",
    "webhook": "node scripts/cdp-webhook-manager.js",
    "webhook:list": "node scripts/cdp-webhook-manager.js list",
    "webhook:create": "node scripts/cdp-webhook-manager.js create",
    "webhook:update": "node scripts/cdp-webhook-manager.js update",
    "webhook:delete": "node scripts/cdp-webhook-manager.js delete",
    "webhook:monitor": "node scripts/cdp-webhook-manager.js monitor",
    "webhook:activate": "node scripts/cdp-webhook-manager.js activate",
    "webhook:deactivate": "node scripts/cdp-webhook-manager.js deactivate",
    "cf:login": "wrangler login",
    "cf:whoami": "wrangler whoami",
    "cf:tail": "wrangler tail",
    "preview": "npm run build && wrangler pages dev",
    "deploy": "npm run deploy:worker && npm run deploy:pages",
    "test:worker": "jest --config jest.config.js",
    "test:frontend": "jest --config jest.config.frontend.js",
    "test:edge": "jest --config jest.config.frontend.js --testMatch='**/*.edge.test.*'"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
